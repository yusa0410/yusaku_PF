<h1>マイページ編集画面</h1>
<%= form_with model: @user, url:user_path, local:true do |f| %>

<h4>画像</h4>
<%= f.label :profile_image, "プロフィール画像" %>
<%#= f.attachment_field :profile_image_id %>
<h4>ユーザー名</h4>
<%= f.text_field :username %>
<h4>プロフィール</h4>
<%= f.text_area :introduction %>
<%= f.submit 'Update User',class: "btn-secondary" %>

<% end %>
